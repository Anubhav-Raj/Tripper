<%- include("./includes/head.ejs") %> <%- include("./includes/navigation.ejs")%>
<link href="/css/Navigation.css" rel="stylesheet" type="text/css" />
<div class="breadcrumb-area pt-5">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="breadcrumb-wrap">
          <h2>Welcome to <strong>TRIPPER</strong></h2>
          <p class="lead fw-normal text-white mb-0">
            Blogs from our <strong>Guides</strong>
          </p>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Package   -->
<div
  class="package-sidebar-wrapper pt-120"
  style="background: #eaeef7; overflow: hidden"
>
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="row">
          <% for( var i=0; i<6; i++){ %>
          <div class="col-lg-4 col-md-6 col-sm-6">
            <div
              class="blog-card"
              style="
                color: #353767 !important;
                border-radius: 20px;
                background-color: #eaeef7;
              "
            >
              <div class="blog-img">
                <img
                  style="height: 200px"
                  src="/img/productHeading.png"
                  alt=""
                  class="img-fluid"
                />
                <div
                  style="background: #353767 !important; color: white"
                  class="blog-date"
                >
                  <i
                    class="fa fa-calendar"
                    style="color: white"
                    aria-hidden="true"
                  ></i
                  >&nbsp; 1 April, 2021
                </div>
              </div>
              <div class="blog-details">
                <div class="blog-info" style="color: #353767 !important">
                  <a
                    class="blog-writer"
                    href="/blog#"
                    style="color: #353767 !important"
                    ><i
                      class="fa fa-calendar"
                      aria-hidden="true"
                      style="color: #353767 !important"
                    ></i
                    >&nbsp;1 April, 2021</a
                  ><a class="blog-comment" href="/blog#"
                    ><i
                      class="fa fa-commenting-o"
                      aria-hidden="true"
                      style="color: #353767 !important"
                    ></i
                    ><span>(5)</span>Comment</a
                  >
                </div>
                <a
                  style="color: #353767 !important"
                  class="blog-title"
                  href="/blog-details"
                  >Donec Egestas Orci Viverra Fermentum Risus.</a
                >
                <div class="package-info mb-2">
                  Lorem ipsum dolor sit amet consectetur adipisicing elit. Cum
                  ut repellendus fuga doloremque, suscipit labore non. Ea
                  architecto itaque assumenda doloremque accusamus quas.
                </div>
                <div class="blog-btn">
                  <a
                    style="background-color: #eaeef7; border: 1px solid #353767"
                    class="btn-common-sm"
                    href="/blogdetails"
                    >Read More</a
                  >
                </div>
              </div>
            </div>
          </div>
          <% } %>
        </div>
      </div>
    </div>
  </div>
</div>
<%- include("./includes/end.ejs") %>
